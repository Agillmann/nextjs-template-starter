{
  "name": "starter-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "lint": "next lint",
    "format": "prettier --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
    "cy": "cypress open",
    "cy:run": "cypress run",
    "e2e:test": "start-server-and-test start http://localhost:3000 cy:run",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.4.0",
    "@mui/x-data-grid": "^5.4.0",
    "axios": "^0.25.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "highcharts": "^9.3.3",
    "highcharts-react-official": "^3.1.0",
    "modern-normalize": "^1.1.0",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "swr": "^1.2.1",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.10",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "cypress": "^9.4.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ]
}
